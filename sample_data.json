{
  "project": "Azure Semantic Kernel Demo",
  "version": "1.0.0",
  "description": "HandoffOrchestration with file upload support",
  "features": [
    "Multi-agent orchestration",
    "Image analysis",
    "File upload and analysis",
    "Dynamic agent handoff"
  ],
  "agents": {
    "SupportAgent": "Initial customer request handler",
    "RefundAgent": "Handles refund requests",
